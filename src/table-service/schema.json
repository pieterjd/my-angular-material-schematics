{
  "properties": {
    "entityName": {
      "type": "string",
      "minLength": 1,
      "x-prompt": "What is the name of the entity? (Eg. an entity of user results in a userlist component and user service)"
    },
    "backendUrl": {
      "type": "string",
      "minLength": 1,
      "x-prompt": "What is the backend url?"
    },
    "servicePath": {
      "type": "string",
      "minLength": 1,
      "default": "",
      "x-prompt": "path to service?"
    }
  },
  "required": ["entityName","backendUrl"]
}
